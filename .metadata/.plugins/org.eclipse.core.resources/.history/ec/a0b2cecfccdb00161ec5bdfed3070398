#include "sensors.h"

void GPS_on(void)
{
	GPIOPinWrite(GPIOA1_BASE, GPIO_PIN_1, 0x00); //Turn on GPS
}

void GPS_off(void)
{
	GPIOPinWrite(GPIOA1_BASE, GPIO_PIN_1, 0xFF); //Turn off GPS
}

void calc_msg_checksum (int* CK_A, int* CK_B){
	int I
	CK_A = 0, CK_B = 0
	for(I=0;I<N;I++){
		CK_A = CK_A + Buffer[I]
		CK_B = CK_B + CK_A
	}
}

