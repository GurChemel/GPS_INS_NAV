#ifndef MAIN_H_
#define MAIN_H_

enum {
	X_pos = 0,
	Y_pos = 1,
	Z_pos = 2
};

#define MAX_STRING_LENGTH 1000
#define DIM_SIZE 3

typedef struct {
	double Px;
	double Py;
	double Pz;
	double Vx;
	double Vy;
	double Vz;
	double Roll;
	double Pitch;
	double Yaw;
} system_state_str;

// TODO: structs should come from AHIAD.
typedef struct{
	double Hx;
	double Hy;
	double Hz;
} mag_input_data_str;
typedef struct{
	double Ax;
	double Ay;
	double Az;
} acc_input_data_str;
typedef struct{
	double Wr;
	double Wp;
	double Wy;
} gyr_input_data_str;
typedef struct{
	double X;
	double Y;
	double Z;
} gps_input_data_str;
// END: structs should come from AHIAD.


#endif /* MAIN_H_ */
