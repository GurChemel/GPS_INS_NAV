}



void calc_msg_checksum (int* CK_A, int* CK_B, int msg_body, int msg_bit_len){
	int I;
	CK_A = 0, CK_B = 0
	for(I=0;I<msg_bit_len;I++){
		CK_A = CK_A + (msg_body & 0x1);
		CK_B = CK_B + CK_A;
		msg_body = (msg_body >> 1);
	}
}

