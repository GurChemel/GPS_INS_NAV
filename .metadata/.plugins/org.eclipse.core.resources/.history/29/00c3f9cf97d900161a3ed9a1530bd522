void get_mag_data(mag_input_data_str* mag_input);
void get_gyr_data(gyr_input_data_str* gyr_input);
void get_acc_data(acc_input_data_str* acc_input);
void get_gps_data(gps_input_data_str* gps_input);
void init_board_and_sensors();


typedef struct {
	double X;
	double Y;
	double Z;
} gps_input_data_str;

typedef struct {
	double Hx;
	double Hy;
	double Hz;
} mag_input_data_str;

typedef struct {
	double Ax;
	double Ay;
	double Az;
} acc_input_data_str;

typedef struct {
	double Wr;
	double Wy;
	double Wp;
} gyr_input_data_str;

